{
  "name": "fastify-oauth-api",
  "version": "1.0.0",
  "description": "Production-ready Fastify v5 OAuth API with PostgreSQL, Redis, and Docker orchestration",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "postinstall": "cd admin && npm install",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:admin\" --names \"API,ADMIN\" --prefix-colors \"cyan,magenta\" --kill-others",
    "dev:api": "dotenvx run -- tsx watch src/server.ts",
    "dev:admin": "cd admin && npm run dev",
    "build": "npm run build:admin && tsc",
    "build:prod": "npm run build:admin && tsup",
    "build:admin": "cd admin && npm run build",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "dotenvx run -- tsx src/db/seeds/index.ts",
    "db:seed:dev": "dotenvx run -- tsx src/db/seeds/development-users.ts",
    "db:seed:superadmin": "dotenvx run -- tsx src/db/seeds/super-admin.ts",
    "dev:token": "dotenvx run -- tsx src/utils/dev-token-generator.ts",
    "docker:start": "bash scripts-docker/start.sh",
    "docker:stop": "bash scripts-docker/system/stop-all.sh",
    "docker:restart": "bash scripts-docker/system/restart-all.sh",
    "docker:health": "bash scripts-docker/system/health-check.sh",
    "docker:logs": "bash scripts-docker/system/logs-all.sh",
    "docker:remove": "bash scripts-docker/system/remove-all.sh",
    "docker:remove:volumes": "bash scripts-docker/system/remove-volumes.sh",
    "docker:setup:swap": "bash scripts-docker/system/setup-swap.sh",
    "docker:postgres": "bash scripts-docker/postgres/run.sh",
    "docker:postgres:stop": "bash scripts-docker/postgres/stop.sh",
    "docker:postgres:log": "bash scripts-docker/postgres/log.sh",
    "docker:postgres:exec": "bash scripts-docker/postgres/exec.sh",
    "docker:postgres:backup": "bash scripts-docker/postgres/backup.sh",
    "docker:postgres:remove": "bash scripts-docker/postgres/remove.sh",
    "docker:redis": "bash scripts-docker/redis/run.sh",
    "docker:redis:stop": "bash scripts-docker/redis/stop.sh",
    "docker:redis:log": "bash scripts-docker/redis/log.sh",
    "docker:redis:exec": "bash scripts-docker/redis/exec.sh",
    "docker:redis:remove": "bash scripts-docker/redis/remove.sh",
    "docker:api": "bash scripts-docker/api/run.sh",
    "docker:api:stop": "bash scripts-docker/api/stop.sh",
    "docker:api:log": "bash scripts-docker/api/log.sh",
    "docker:api:exec": "bash scripts-docker/api/exec.sh",
    "docker:api:rebuild": "bash scripts-docker/api/rebuild.sh",
    "docker:api:remove": "bash scripts-docker/api/remove.sh",
    "docker:caddy": "bash scripts-docker/caddy/run.sh",
    "docker:caddy:stop": "bash scripts-docker/caddy/stop.sh",
    "docker:caddy:log": "bash scripts-docker/caddy/log.sh",
    "docker:caddy:exec": "bash scripts-docker/caddy/exec.sh",
    "docker:caddy:reload": "bash scripts-docker/caddy/reload.sh",
    "docker:caddy:remove": "bash scripts-docker/caddy/remove.sh",
    "test": "dotenvx run -- vitest",
    "test:ui": "dotenvx run -- vitest --ui",
    "test:run": "dotenvx run -- vitest run",
    "test:coverage": "dotenvx run -- vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "fastify",
    "oauth",
    "google-signin",
    "apple-signin",
    "jwt",
    "postgresql",
    "redis",
    "docker",
    "typescript",
    "drizzle-orm",
    "api",
    "rest-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/auth": "^5.0.3",
    "@fastify/autoload": "^6.0.3",
    "@fastify/bearer-auth": "^10.0.2",
    "@fastify/compress": "^8.0.3",
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/csrf-protection": "^7.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/oauth2": "^8.1.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/redis": "^7.1.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@sinclair/typebox": "^0.34.41",
    "@types/bcryptjs": "^2.4.6",
    "apple-signin-auth": "^2.0.0",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.44.6",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^6.0.0",
    "google-auth-library": "^10.4.1",
    "ioredis": "^5.4.2",
    "nanoid": "^5.1.6",
    "pino": "^10.0.0",
    "postgres": "^3.4.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.27.1",
    "@eslint/js": "^9.20.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.6",
    "@vitest/coverage-v8": "^3.0.6",
    "@vitest/ui": "^3.0.6",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.20.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0",
    "vitest": "^3.0.6"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "overrides": {
    "esbuild": "^0.25.11"
  }
}
